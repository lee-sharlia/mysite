<div class="post-col homepage_content">
    <div class="post-container"
        <%= @post.title %><br>
        <%= @post.content %> 

        <br>
        <br>

        <%= link_to "Edit", edit_post_path(@post) %>
        <br>
        <%= link_to "Delete", post_path(@post), method: :delete, data: {confirm: "Are you sure you want to delete this post?"} %>

    </div>
</div>